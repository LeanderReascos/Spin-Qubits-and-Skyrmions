   "source": [
    "from qutip import destroy, qeye, tensor, mesolve"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# | n, s>\n",
    "a = tensor(destroy(3), qeye(2))\n",
    "ad = a.dag()\n",
    "\n",
    "mesolve()\n",
    "\n",
    "H_dependent =  - e* E0 * (ad + a)\n",
    "f = lamnda t: cos(t)\n",
    "\n",
    "\n",
    "[H_independe, [H_dependent, f(t)]]"
   ]
  }
